class Person \{\
  //init varaibles when creating a class with constructor\
  Person(\{this.name, this.age, this.height\});//Constructor same name as class\
  final String name;\
  int age;\
  double height;\
  String sayName() => name; //arrow inplace of \{\} and return\
\
   @override //toString is a method from parent root class Object\
  String toString() => 'name: $name, age: $age, height: $height'; //override toString from Object root class\
\
\}  \
